<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wsk.backend3.mapper.SetClubApplyMapper">

    <resultMap id="BaseResultMap" type="com.wsk.backend3.domain.SetClubApply">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="category" column="category" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="userid" column="userId" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,category,name,
        userId
    </sql>
    <select id="selectSCA" resultType="com.wsk.backend3.dto.SetClubAddmissionInfoDto">
        select stu.true_name,set_club_apply.name,set_club_apply.category from set_club_apply,stu
        where set_club_apply.userId = stu.id
    </select>
</mapper>
